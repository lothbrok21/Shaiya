push summon_dest_map_name
mov ecx,008655E8
call 004D2E90
pop ecx

push summon_dest_map_name
push 021FE604
push dialogue_text
push dialogue_text_box
call sprintf
add esp,10
mov eax,dialogue_text_box
jmp 00528E8D

read_summon_packet:
push 4
lea eax,[esp+4]
push eax
call 0059FB80

mov ecx,[esp+8]
