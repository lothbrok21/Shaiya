summon_dest_map_id:
dd 0

00528E7F:
jmp write_summon_destination

00596265:
jmp read_summon_packet

[DISABLE]
</AssemblerScript>
    </CheatEntry>
    <CheatEntry>
      <ID>0</ID>
      <Description>"Server Side"</Description>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>{
  Date   : 27.06.2022
  Author : Lothbrok
  Credits : Cups
}
[ENABLE]
alloc(re_write_summon_packet,2048)

re_write_summon_packet:
push 08 { Packet size }
lea ecx,[esp+2C]
add eax,000088B8
push ax
mov ax,word ptr[edi+160] { edi : Player sending the summon. }
mov word ptr[ecx+6],ax
pop ax
push ecx { Packet }
mov ecx,esi
mov [esi+00001498],edx
mov [esi+0000149C],eax
call 004ED0E0 { SConnection::Send }
jmp 0049E513

0049E4F4:
jmp re_write_summon_packet
nop
returnhere:
 
[DISABLE]
dealloc(re_write_summon_packet)
